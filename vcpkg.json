{
    "name": "image-library",
    "version": "0.1-alpha",
    "homepage": "https://github.com/anirul/image-library",
    "description": "An image independent simple library.",
    "dependencies": [
        "abseil",
        "glm",
        "glew",
        "gtest",
        "happly",
        {
        "name": "imgui",
        "features": [ "opengl3-binding", "sdl2-binding", "docking-experimental" ]
        },
        "opencv",
        "opengl",
        "protobuf",
        {
        "name": "sdl2",
        "features": [ "vulkan" ]
        },
        "spdlog",
        "stb",
        "tinyobjloader",
        "vulkan"
    ]
}